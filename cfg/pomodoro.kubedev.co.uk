server {
        listen 80;
        listen [::]:80;

        root /home/user01/app-pomodoro/front/public;
        index index.html index.htm index.nginx-debian.html;

        server_name pomodoro.kubedev.co.uk www.pomodoro.kubedev.co.uk;

        location / {
            try_files $uri $uri/ =404;
        }
}