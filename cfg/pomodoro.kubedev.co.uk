server {
        listen 80;
        listen [::]:80;

        root /var/www/html;
        index index.html index.htm index.nginx-debian.html;

        server_name pomodoro.kubedev.co.uk www.pomodoro.kubedev.co.uk;

        error_log /var/log/nginx/error.log debug;

        location / {
            try_files $uri $uri/ =404;
        }
}